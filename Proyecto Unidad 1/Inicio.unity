  m_Script: {fileID: 11500000, guid: e779930f26b6aef47be716dc0bbf4ee9, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  charla:
  - name: Sofia
    texto: "<color=#FF5733><b>\xA1Hola</b></color> <color=#33FF57><i>chicos</i></color>
      <color=#3357FF><u>\xBFC\xF3mo</u></color> <color=#FF33A1>est\xE1n?</color>  "
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Eduardo
    texto: "<color=#FF6347><b>\xA1Hola</b></color>, <color=#4682B4><b>Sof\xEDa!</b></color>
      <color=#32CD32><b>Bien</b></color>, <color=#FFD700><b>gracias</b></color>.
      <color=#8A2BE2><b>\xBFY t\xFA?</b></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Alan
    texto: "<color=#FF4500><b>\xA1Hola!</b></color> <color=#1E90FF><b>Todo</b></color>
      <color=#32CD32><b>bien</b></color> <color=#FFD700><b>por</b></color> <color=#8A2BE2><b>aqu\xED</b></color>
      <color=#DA70D6><b>tambi\xE9n</b></color>."
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Sofia
    texto: "<color=#FF6347><b>Estoy</b></color> <color=#4682B4><i>bien,</i></color>
      <color=#32CD32><u>gracias.</u></color> <color=#FFD700><b><i>Estaba</i></b></color>
      <color=#8A2BE2><u><b>pensando</b></u></color> <color=#DA70D6><i>en</i></color>
      <color=#FF1493><b><u>planear</u></b></color> <color=#00FA9A><b>un</b></color>
      <color=#FF8C00><u>viaje</u></color> <color=#4B0082><i>juntos.</i></color> <color=#00BFFF><b>\xBFQu\xE9</b></color>
      <color=#C71585><u><i>opinan?</i></u></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: a1515f9f88663934a8bda5fc6dab5bcb, type: 3}
  - name: Eduardo
    texto: "<color=#FF5733><b>\xA1Me</b></color> <color=#33FF57><i>encanta</i></color>
      <color=#3357FF><u>la</u></color> <color=#F7E700><b><i>idea!</i></b></color>
      <color=#FF33A1><u><b>\xBFA</b></u></color> <color=#7D33FF><i>d\xF3nde</i></color>
      <color=#FF6F33><b><u>podr\xEDamos</u></b></color> <color=#33F4FF><b>ir?</b></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: a1515f9f88663934a8bda5fc6dab5bcb, type: 3}
  - name: Alan
    texto: "<color=#FF6F61><b>S\xED,</b></color> <color=#6B8E23><i>suena</i></color>
      <color=#1E90FF><u>genial.</u></color> <color=#FF1493><b><i>Siempre</i></b></color>
      <color=#00BFFF><u><b>he</b></u></color> <color=#32CD32><i>querido</i></color>
      <color=#FF4500><b><u>ir</u></b></color> <color=#9932CC><b>a</b></color> <color=#FFD700><u>la</u></color>
      <color=#FF6347><i>playa.</i></color>"
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 1bc7b119855468e448df6f2ffb1eec20, type: 3}
  - name: Sofia
    texto: "<color=#FF8C00><b>\xA1Perfecto!</b></color> <color=#8A2BE2><i>\xBFQu\xE9</i></color>
      <color=#00FA9A><u>tal</u></color> <color=#FF1493><b><i>si</i></b></color> <color=#20B2AA><u><b>vamos</b></u></color>
      <color=#FF4500><i>a</i></color> <color=#00BFFF><b><u>Canc\xFAn</u></b></color>
      <color=#DA70D6><i>?</i></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: ec41ac00d0722ca4db259927a54fcbc7, type: 3}
  - name: Eduardo
    texto: "<color=#FF6347><b>Canc\xFAn</b></color> <color=#4682B4><i>suena</i></color>
      <color=#32CD32><u>incre\xEDble.</u></color> <color=#FFD700><b><i>Playas</i></b></color>
      <color=#8A2BE2><u><b>hermosas</b></u></color> <color=#DA70D6><i>y</i></color>
      <color=#FF1493><b><u>buena</u></b></color> <color=#00FA9A><b>comida.</b></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 0c0e4b2edebdee34484ba1c6b077f3cb, type: 3}
  - name: Alan
    texto: "<color=#FF7F50><b>Adem\xE1s,</b></color> <color=#4682B4><i>podemos</i></color>
      <color=#32CD32><u>visitar</u></color> <color=#8B4513><b><i>las</i></b></color>
      <color=#9370DB><u><b>ruinas</b></u></color> <color=#FF4500><i>mayas.</i></color>
      <color=#2E8B57><b><u>Ser\xEDa</u></b></color> <color=#FFD700><b>una</b></color>
      <color=#6A5ACD><i>experiencia</i></color> <color=#FF6347><u>cultural</u></color>
      <color=#DA70D6><b><i>interesante.</i></b></color>"
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: f7a14f6bd151d50468226fa83e2dd729, type: 3}
  - name: Sofia
    texto: "<color=#FF69B4><b>\xA1Exacto!</b></color> <color=#5F9EA0><i>Y</i></color>
      <color=#FF8C00><u>tambi\xE9n</u></color> <color=#7B68EE><b><i>hay</i></b></color>
      <color=#32CD32><u><b>muchas</b></u></color> <color=#DC143C><i>actividades</i></color>
      <color=#FFD700><b>acu\xE1ticas.</b></color> <color=#4682B4><b><i>\xBFCu\xE1ndo</i></b></color>
      <color=#FF4500><u><b>podr\xEDamos</b></u></color> <color=#00FA9A><b>ir?</b></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: e86120080d6ab1a4a92705ee7c6608fc, type: 3}
  - name: Eduardo
    texto: "<color=#FF4500><b>Yo</b></color> <color=#1E90FF><i>estoy</i></color>
      <color=#32CD32><u>libre</u></color> <color=#8A2BE2><b><i>el</i></b></color>
      <color=#FFD700><u><b>pr\xF3ximo</b></u></color> <color=#FF69B4><i>mes.</i></color>
      <color=#00FA9A><b><u>\xBFQu\xE9</u></b></color> <color=#DC143C><i>tal</i></color>
      <color=#4682B4><b>ustedes?</b></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Alan
    texto: "<color=#FF6347><b>Yo</b></color> <color=#4682B4><i>tambi\xE9n</i></color>
      <color=#32CD32><u>estoy</u></color> <color=#FFD700><b><i>disponible</i></b></color>
      <color=#8A2BE2><u><b>el</b></u></color> <color=#FF69B4><i>pr\xF3ximo</i></color>
      <color=#20B2AA><b>mes.</b></color>"
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Sofia
    texto: "<color=#FF7F50><b>Entonces,</b></color> <color=#6A5ACD><i>\xA1ya</i></color>
      <color=#32CD32><u>tenemos</u></color> <color=#FFD700><b><i>fecha!</i></b></color>
      <color=#4682B4><u><b>\xBFCu\xE1nto</b></u></color> <color=#FF4500><i>tiempo</i></color>
      <color=#8A2BE2><b>nos</b></color> <color=#20B2AA><u>quedamos?</u></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Eduardo
    texto: "<color=#FF6347><b>\xBFQu\xE9</b></color> <color=#6A5ACD><i>les</i></color>
      <color=#32CD32><u>parece</u></color> <color=#FFD700><b><i>una</i></b></color>
      <color=#4682B4><u><b>semana?</b></u></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Alan
    texto: <color=#FF4500><b>Una</b></color> <color=#1E90FF><i>semana</i></color>
      <color=#32CD32><u>suena</u></color> <color=#FFD700><b><i>perfecto.</i></b></color>
      <color=#6A5ACD><u>Suficiente</u></color> <color=#FF6347><i>tiempo</i></color>
      <color=#8A2BE2><b>para</b></color> <color=#4682B4><u><i>relajarnos</i></u></color>
      <color=#20B2AA><b>y</b></color> <color=#DC143C><u><b>explorar.</b></u></color>
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Sofia
    texto: "<color=#FF7F50><b>De</b></color> <color=#6A5ACD><i>acuerdo.</i></color>
      <color=#32CD32><u>Voy</u></color> <color=#FFD700><b><i>a</i></b></color> <color=#4682B4><u>buscar</u></color>
      <color=#FF6347><i>opciones</i></color> <color=#8A2BE2><b>de</b></color> <color=#20B2AA><u>hospedaje.</u></color>
      <color=#FF4500><b>\xBFPrefieren</b></color> <color=#1E90FF><i>un</i></color>
      <color=#32CD32><u>hotel</u></color> <color=#FFD700><b><i>o</i></b></color>
      <color=#6A5ACD><u>una</u></color> <color=#4682B4><b><u>casa?</u></b></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Eduardo
    texto: "<color=#DC143C><b>Creo</b></color> <color=#20B2AA><i>que</i></color>
      <color=#FF6347><u>una</u></color> <color=#1E90FF><b>casa</b></color> <color=#32CD32><i>ser\xEDa</i></color>
      <color=#FFD700><b><u>m\xE1s</u></b></color> <color=#6A5ACD><i>c\xF3modo</i></color>
      <color=#8A2BE2><b>y</b></color> <color=#FF4500><u>privado.</u></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Alan
    texto: "<color=#FF4500><b>S\xED,</b></color> <color=#7FFF00><i>adem\xE1s</i></color>
      <color=#FF6347><u>podr\xEDamos</u></color> <color=#1E90FF><b><i>cocinar</i></b></color>
      <color=#32CD32><u>algunas</u></color> <color=#FFD700><i>comidas</i></color>
      <color=#6A5ACD><b>y</b></color> <color=#FF1493><u>ahorrar</u></color> <color=#00FA9A><b><i>un</i></b></color>
      <color=#8A2BE2><u>poco.</u></color>"
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Sofia
    texto: "<color=#FF69B4><b>Me</b></color> <color=#40E0D0><i>parece</i></color>
      <color=#DA70D6><u>bien.</u></color> <color=#FF8C00><b><i>Buscar\xE9</i></b></color>
      <color=#00BFFF><u>algunas</u></color> <color=#FFD700><i>opciones</i></color>
      <color=#FF4500><b>y</b></color> <color=#6A5ACD><u>les</u></color> <color=#20B2AA><b><i>mando</i></b></color>
      <color=#FF6347><u>las</u></color> <color=#00FA9A><b>propuestas.</b></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Eduardo
    texto: <color=#FF4500><b>Perfecto.</b></color> <color=#8A2BE2><i>Yo</i></color>
      <color=#32CD32><u>me</u></color> <color=#00BFFF><b><i>encargo</i></b></color>
      <color=#FFD700><u>de</u></color> <color=#FF6347><i>ver</i></color> <color=#6A5ACD><b>los</b></color>
      <color=#20B2AA><u>vuelos.</u></color>
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 0f296123a91fea44cb6b66b6f42f4d67, type: 3}
  - name: Alan
    texto: "<color=#FF69B4><b>Yo</b></color> <color=#00FA9A><i>puedo</i></color>
      <color=#FF6347><u>investigar</u></color> <color=#20B2AA><b><i>sobre</i></b></color>
      <color=#FFD700><u>las</u></color> <color=#32CD32><i>actividades</i></color>
      <color=#FF4500><b>que</b></color> <color=#6A5ACD><u>podr\xEDamos</u></color>
      <color=#8A2BE2><i>hacer</i></color> <color=#1E90FF><b>all\xE1.</b></color>"
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Sofia
    texto: "<color=#FF4500><b>\xA1S\xFAper</b></color> <color=#00CED1><i>equipo!</i></color>
      <color=#FFD700><u>\xBFTienen</u></color> <color=#7FFF00><b><i>alguna</i></b></color>
      <color=#FF6347><u>actividad</u></color> <color=#1E90FF><i>espec\xEDfica</i></color>
      <color=#32CD32><b>en</b></color> <color=#8A2BE2><u>mente?</u></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Eduardo
    texto: "<color=#FF1493><b>Me</b></color> <color=#8A2BE2><i>gustar\xEDa</i></color>
      <color=#32CD32><u>hacer</u></color> <color=#00BFFF><b><i>snorkel</i></b></color>
      <color=#FFD700><u>en</u></color> <color=#FF6347><i>los</i></color> <color=#20B2AA><b>arrecifes.</b></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: caa4b0a21141be6429ace581a373b27f, type: 3}
  - name: Alan
    texto: <color=#FF6347><b>Yo</b></color> <color=#00FA9A><i>quiero</i></color>
      <color=#DA70D6><u>explorar</u></color> <color=#20B2AA><b><i>las</i></b></color>
      <color=#FFD700><u>ruinas</u></color> <color=#FF1493><i>mayas,</i></color> <color=#8A2BE2><b>definitivamente.</b></color>
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: f7a14f6bd151d50468226fa83e2dd729, type: 3}
  - name: Sofia
    texto: "<color=#FF4500><b>Me</b></color> <color=#00CED1><i>encantar\xEDa</i></color>
      <color=#32CD32><u>hacer</u></color> <color=#FF6347><b><i>una</i></b></color>
      <color=#1E90FF><u>excursi\xF3n</u></color> <color=#FFD700><i>en</i></color>
      <color=#8A2BE2><b>bote.</b></color> <color=#FF8C00><u>He</u></color> <color=#00FA9A><i>o\xEDdo</i></color>
      <color=#FF6347><b>que</b></color> <color=#20B2AA><u>hay</u></color> <color=#8A2BE2><i>tours</i></color>
      <color=#FF4500><b>para</b></color> <color=#00BFFF><u>nadar</u></color> <color=#FFD700><i>con</i></color>
      <color=#00CED1><b>delfines.</b></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 8bb581e1635301d40a890954d99673a9, type: 3}
  - name: Eduardo
    texto: "<color=#FF69B4><b>\xA1Nadar</b></color> <color=#FF6347><i>con</i></color>
      <color=#00FA9A><u>delfines</u></color> <color=#32CD32><b><i>suena</i></b></color>
      <color=#1E90FF><u>incre\xEDble!</u></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 8bb581e1635301d40a890954d99673a9, type: 3}
  - name: Alan
    texto: "<color=#FF4500><b>S\xED,</b></color> <color=#00FA9A><i>eso</i></color>
      <color=#FFD700><u>definitivamente</u></color> <color=#1E90FF><b><i>tiene</i></b></color>
      <color=#FF6347><u>que</u></color> <color=#32CD32><i>estar</i></color> <color=#FF8C00><b>en</b></color>
      <color=#20B2AA><u>nuestra</u></color> <color=#8A2BE2><i>lista.</i></color>"
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Sofia
    texto: "<color=#FF6347><b>Tambi\xE9n</b></color> <color=#00FA9A><i>podr\xEDamos</i></color>
      <color=#FFD700><u>dedicar</u></color> <color=#32CD32><b><i>un</i></b></color>
      <color=#1E90FF><u>d\xEDa</u></color> <color=#FF8C00><i>a</i></color> <color=#20B2AA><b>relajarnos</b></color>
      <color=#FF1493><u>en</u></color> <color=#00BFFF><i>la</i></color> <color=#FF4500><b>playa</b></color>
      <color=#8A2BE2><u>sin</u></color> <color=#00CED1><i>hacer</i></color> <color=#FF6347><b>nada.</b></color>"
    image: {fileID: 21300000, guid: d940cd9b452d4ff43a7af22219e7b89d, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 1bc7b119855468e448df6f2ffb1eec20, type: 3}
  - name: Eduardo
    texto: "<color=#FF5733><b>Un</b></color> <color=#C70039><i>d\xEDa</i></color>
      <color=#900C3F><u>de</u></color> <color=#581845><b>descanso</b></color> <color=#FF6F61><i>total</i></color>
      <color=#FFB6C1><u>suena</u></color> <color=#FF8C00><b>bien.</b></color> <color=#00FA9A><i>A</i></color>
      <color=#7FFF00><u>veces</u></color> <color=#20B2AA><b>es</b></color> <color=#00CED1><i>necesario.</i></color>"
    image: {fileID: 21300000, guid: aceb2b0311ea1bb4283822ea90b42418, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
  - name: Alan
    texto: <color=#FF6347><b>Totalmente</b></color> <color=#4682B4><i>de</i></color>
      <color=#32CD32><u>acuerdo.</u></color> <color=#FF4500><b>No</b></color> <color=#00FA9A><i>todo</i></color>
      <color=#8A2BE2><u>tiene</u></color> <color=#FF8C00><b>que</b></color> <color=#20B2AA><i>ser</i></color>
      <color=#FF1493><u>actividad</u></color> <color=#00CED1><b>tras</b></color>
      <color=#FF6F61><i>actividad.</i></color>
    image: {fileID: 21300000, guid: f25ce8d32851b0d4b8a089b52abf458e, type: 3}
    fondo_conversacion: {fileID: 21300000, guid: 87e1396a526959f4293c71e045632ad3, type: 3}
